{
    "name": "nobox/lazy-strings-lumen",
    "description": "Lumen service provider for LazyStrings.",
    "license" : "MIT",
    "keywords": ["lumen", "googledocs", "spreadsheet"],
    "homepage": "https://github.com/Nobox/lazy-strings-lumen",
    "authors": [
        {
            "name": "Alberto Estrada",
            "email": "alberto@nobox.com"
        },
        {
            "name": "Antonio Rodriguez",
            "email": "antonio@nobox.com"
        },
        {
            "name": "Jonathan Torres",
            "email": "jonathan@nobox.com"
        },
        {
            "name": "Waldemar Figueroa",
            "email": "waldemar@nobox.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.1.*",
        "illuminate/console": "5.1.*",
        "nobox/lazy-strings": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Nobox\\LazyStrings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nobox\\LazyStrings\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "5.4"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable"
}
